      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="mortgage" id="BPMNShape_mortgage">
        <omgdc:Bounds height="55.0" width="105.0" x="990.0" y="213.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="makeLoans" id="BPMNShape_makeLoans">
